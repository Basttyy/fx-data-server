{
    "name": "basttyy/fx-data-server",
    "description": "fx historical data server",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "../atomframework",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "eyika/atom-framework": "dev-dev",
        "nesbot/carbon": "^3.7",
        "monolog/monolog": "^3.7",
        "predis/predis": "^2.2",
        "vlucas/phpdotenv": "^5.6",
        "phpmailer/phpmailer": "^6.9",
        "robmorgan/phinx": "^0.16.2",
        "guzzlehttp/guzzle": "^7.9",
        "dragonmantank/cron-expression": "^3.3",
        "eftec/bladeone": "^4.13",
        "eftec/bladeonehtml": "^2.4",
        "ip2location/ip2location-php": "^9.7",
        "ezyang/htmlpurifier": "^4.17",
        "firebase/php-jwt": "^6.10",
        "pragmarx/google2fa": "^8.0",
        "hybridauth/hybridauth": "^3.11",
        "gregwar/captcha": "^1.2",
        "ratchet/pawl": "^0.4.1",
        "cboden/ratchet": "^0.4.4",
        "league/flysystem": "^3.28"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "fakerphp/faker": "^1.23"
    },
    "license": "MIT",
    "scripts": {
        "post-autoload-dump": [
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=atom-assets --ansi --force"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Abdulbasit",
            "email": "basttyy@gmail.com"
        }
    ],
    "minimum-stability": "beta"
}
